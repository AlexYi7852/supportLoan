/*=============================================
=            引入lib啊、样式            =
=============================================*/

@import './libs/wxss/weui.wxss';
/*=============================================
=            公共模块           =
=============================================*/

page,
view,
.container {
  box-sizing: border-box;
}

page,
.container {
  min-height: 100vh;
}

page {
  font-size: 14px;
}

.container {
  overflow-x: hidden;
}

image {
  vertical-align: middle;
}

.container {
  background-color: #f3f4f4;
  color: #1e211f;
  position: relative;
}

input {
  height: 100%;
}


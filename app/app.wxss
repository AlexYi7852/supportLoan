/*=============================================
=            weui extend            =
=============================================*/

.weui-cell:before {
  right: 15px;
  border-top-color: #e4e9e7;
}

.weui-cell-1 {
  padding: 0;
  align-items: flex-start;
}

.weui-cell-1::before {
  display: none;
}

.weui-cell-2 {
  padding: 0;
}

.weui-cell-2::before {
  display: none;
}

.weui-cell__ft_in-access:after {
  margin-top: -2px;
}

.weui-cell-3 {
  padding: 20rpx 30rpx;
}

.weui-cell-4 {
  padding: 18rpx 0rpx;
}

.weui-cell-4::before {
  display: none;
}

.weui-cell-5::before {
  display: none;
}


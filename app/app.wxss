/*=============================================
=            公共模块            =
=============================================*/

/* badge */

.m-badge {
  background-color: #fff;
}

.m-badge .item {
  padding: 0 30rpx;
}

.m-badge .item .bd {
  padding: 20rpx 0;
  border-bottom: 1rpx solid #e4e9e7;
}

.m-badge .item:last-child .bd {
  border-bottom: none;
}

/*=============================================
=            公共组件            =
=============================================*/

.u-btn {
  display: inline-block;
  text-align: center;
}

.u-btn:active {
  background: #1e7f4c;
}

.u-title::before {
  content: '';
  display: inline-block;
  width: 4rpx;
  height: 30rpx;
  background-color: #07944a;
  margin-right: 14rpx;
  vertical-align: middle;
}

.u-btn-primary {
  border-radius: 300px;
  width: 520rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #07944a;
  color: #fff;
  box-shadow: 0px 5px 18px 0px rgba(3, 200, 97, 0.28);
}

.u-btn-accept,
.u-btn-cancle {
  border-radius: 300px;
  width: 320rpx;
  height: 90rpx;
  line-height: 90rpx;
}

.u-btn-cancle {
  background-color: #fff;
}

.u-btn-accept {
  background-color: #07944a;
  color: #fff;
}

.u-btn-accept1 {
  border-radius: 300px;
  width: 520rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #c3c9c6;
  color: #fff;
}

.u-btn-accept1-active {
  background-color: #07944a;
}

.u-counter {
  position: relative;
  width: 100%;
  height: 84rpx;
  line-height: 84rpx;
  background-color: #f3f6f4;
}

.u-counter .add,
.u-counter .reduce {
  position: absolute;
  top: 0;
  width: 84rpx;
  height: 100%;
  font-size: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #07944a;
  color: #fff;
}

.u-counter .add {
  right: 0;
}

.u-counter .reduce {
  left: 0;
}

.u-counter .input-wrap,
.u-counter .input-wrap input {
  height: 100%;
  text-align: center;
}

.u-counter .input-wrap {
  padding: 0 84rpx;
}

.u-tag {
  display: inline-block;
}

.u-tag-1 {
  padding: 0 20rpx;
  border-radius: 300px;
  background-color: #12ba74;
  color: #fff;
}


/* 申请 */
.container{
  position: relative;
}
.m-apply{
  padding: 0 30rpx;
  background-color: #fff;
}
.m-apply .box-h{
  height: 104rpx;
  line-height: 104rpx;
  font-size: 15px;
}
.m-apply .box-b{
  height: 180rpx;
  display: flex;
  align-items: center;
}
.m-apply .weui-flex{
  flex: 1;
  text-align: center;
  align-items: center;
}

/* 公开信息 */
.m-info{
  padding: 38rpx 30rpx;
  background-color: #fff;
}
.m-info .desc{
  color: #989e9a;
}
/* 额度 */
.m-limit{
  padding: 30rpx;
  background-color: #e7fbf1;
  text-align: center;
}
.m-limit .limit{
  color: #1eb867;
  font-size: 25px;
}

/* 生源地 校园地 */
.m-badge .province{
  display: inline-block;
  min-width: 138rpx;
}
.m-badge .bank{
  color: #989e9a;
  margin-left: 10rpx;
}

/* 新闻公告 */
.m-announce{
  background-color: #fff;
  padding: 60rpx 60rpx 60rpx 30rpx;
}
.icon-question{
 
  width: 40rpx;
  height: 40rpx;
  float: right;
  background: url(data:image/png;base64,);
 background-size: 40rpx 40rpx;
}
.loanType{
  position: fixed;
  width: 750rpx;
  height: 100%;
  z-index: 999;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.8);
}
.loan-content{
  width: 550rpx;
  height: 360rpx;
  margin: 0 auto;
  background: #FFF;
  margin-top: 50%;
  border-radius: 20rpx;
  box-sizing: border-box;
  padding: 40rpx;
  position: relative;

}
.loan-content-title{
  text-align: center;
  line-height: 40rpx;
  font-size: 36rpx;
  margin-bottom: 20rpx;
}
.loan-content .list{
  line-height: 50rpx;
  margin-top: 10rpx;
  font-size: 28rpx;
}
.loan-btn{
  margin-top: 20px;
}
.loan-btn view{
  float: left;
  font-size: 32rpx;
  color:#1eb867;
  padding: 10px 0;
  width: 50%;
  text-align: center
}
.borderLeft {
  border-left:1rpx solid #e3e3e3;
}
.close-btn{
  position: absolute;
  right: 0;
  top: 0;
  width: 40px;
  height: 40px;
  background:url(data:image/png;base64,) center no-repeat;
  background-size: 28rpx 28rpx;
}
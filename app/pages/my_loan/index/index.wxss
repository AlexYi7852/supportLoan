/* 项目 */
.m-nav {
  background-color: #fff;
  padding: 40rpx 0;
}
.m-nav .item .image-wrap {
  position: relative;
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
}


/* 我的申请 */
.m-apply {
  background-color: #fff;
}

/* 贷款信息 */
.m-loan_info {
  padding: 40rpx;
  background-color: #e7fbf1;
}
.m-loan_info .bank-box {
  margin: 0 100rpx 0 20rpx;
}
.m-loan_info .desc {
  color: #535a55;
}

/* 审核反馈 */
.m-feedback {
  background-color: #fff;
}
.m-feedback .header {
  border-bottom: 1px solid #e4e9e7;
}
.m-feedback .footer {
  border-top: 1px solid #e4e9e7;
}

.m-feedback .header-1 {
  border-bottom: none;
}

.m-feedback .footer-1 {
  border: none;
}

.apply-title {
  width: 100%;
  padding: 20rpx;
  background: #fff;
  margin-top: 20rpx;
  height: 50px;
  float: left;
  clear: both;
}
.g-mn {
  width: 100%;
}

.my-loan {
  width: 100%;
  margin-top: 20rpx;
  background: #fff;
  padding: 0 30rpx;
  float: left;
  height: auto;
  overflow: hidden;
}
.loan-title {
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 40rpx;
  border-bottom: 1rpx solid #e3e3e3;
}
.loan-icon {
  background: url('data:image/png;base64,')
    left no-repeat;
  background-size: 34rpx 32rpx;
  font-size: 28rpx;
}
.loan-place {
  padding: 20rpx 0;
}
.place-name {
  font-size: 32rpx;
  font-weight: 700;
}
.bank-name {
  padding-left: 50rpx;
  font-size: 28rpx;
  color: #333;
}
.cell-view {
  width: 100%;
  padding: 5rpx 0;
}
.period text {
  font-size: 32rpx;
}
.color666 {
  color: #666;
}

.period .total {
  color: #666;
  font-size: 32rpx;
}
.m-color {
  color: #07934a;
}
.list-period {
  background-color: #e7fbf1;
  padding: 0 20rpx;
}

.list-view {
  width: 100%;
}
.bank-icon {
  width: 120rpx;
  height: 76rpx;
}
.HRBANK {
  background: url(data:image/png;base64,)
    center no-repeat;
  background-size: 120rpx 76rpx;
}
.PSBC {
  background: url(data:image/png;base64,)
    center no-repeat;
  background-size: 120rpx 76rpx;
}

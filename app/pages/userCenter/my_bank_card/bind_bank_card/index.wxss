/* 银行卡信息 */

.m-bank .add-card {
    width: 100%;
    height: 200rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #f5f7f6;
    text-align: center;
    color: #999f9b;
    border-radius: 8px;
}

.m-bank .card {
    position: relative;
    background-color: #a1569b;
    border-radius: 8px;
    padding: 26rpx 30rpx;
    color: #fff;
}

.m-bank .card-1 {
    background-color: #16aa66;
}

.m-bank .card .g-icon {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}

.m-bank .card .sub {
    font-size: 12px;
}

.m-bank .tip .c-warn {
    color: #ff0000;
}

.m-bank .tip1 {
    color: #989e9a;
    font-size: 13px;
}

.m-bank .change {
    color: #4193d2;
}

/* 添加银行卡弹层 */

.m-pop {
    position: fixed;
    z-index: 3;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(66, 66, 66, .6);
    padding: 30rpx;
}

.m-pop .content-wrap {
    background-color: #fff;
    border-radius: 8px;
    overflow: hidden;
}

.m-pop .header {
    position: relative;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    background-color: #07944a;
    color: #fff;
}

.m-pop .header .g-icon-l {
    position: absolute;
    left: 0;
    top: 0;
    width: 88rpx;
    text-align: center;
}

.m-pop .header .g-icon-r {
    position: absolute;
    right: 0;
    top: 0;
    width: 88rpx;
    text-align: center;
}

.m-pop .content {
    background-color: #fff;
}

.m-pop .content1 {
    padding: 30rpx;
    background-color: #ecfaf3;
    max-height: 600rpx;
    overflow-y: auto;
}

.m-pop .input-wrap {
    position: relative;
    background-color: #f3f6f4;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 100rpx 0 30rpx;
}

.m-pop .input-wrap .input-icon {
    position: absolute;
    right: 0;
    top: 0;
    padding: 0 30rpx;
}

.icon-card {
    background: #FFF;
    height: 84rpx;
    width: 84rpx;
    border-radius: 50%;
    padding: 4rpx;
    box-sizing: content-box
}

.icon {
    width: 84rpx;
    height: 84rpx;
    float: left;
}
.CCB {
    background-color: rgba(82, 145, 210, 1) !important;
}
.CCB .icon
{
    background:url("data:image/png;base64,") center no-repeat;
    background-size: 84rpx 84rpx;
}
/* 邮政储蓄银行 */
.PSBC {
    background-color: rgba(23, 170, 102, 1) !important;
}
.PSBC .icon {
    background: url(data:image/png;base64,) center no-repeat;
    background-size: 84rpx 84rpx;
}
.HRBANK {
    background: #ff6565 !important;
}


.HRBANK .icon {
    background: url(data:image/png;base64,) center no-repeat;
    background-size: 84rpx 84rpx;
